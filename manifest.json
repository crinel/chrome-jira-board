{
    "name": "Jira Late Stories on board",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "See late stories on jira board",
    "background": {
        "scripts": ["js/background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Jira Late Stories on board"
    },
    "permissions": ["contextMenus", "activeTab", "storage"],
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    }
}
